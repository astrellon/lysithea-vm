{
  "name": "lysithea-vm",
  "version": "0.3.9",
  "description": "A simple stack based virtual machine call Lysithea.",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "perfTest": "ts-node --project ./examples/tsconfig.json ./examples/perfTestIndex.ts",
    "diagTree": "ts-node --project ./examples/tsconfig.json ./examples/dialogueTreeIndex.ts",
    "stdLib": "ts-node --project ./examples/tsconfig.json ./examples/standardLibraryIndex.ts",
    "readmeExample": "ts-node --project ./examples/tsconfig.json ./examples/readmeExampleIndex.ts",
    "build": "rm -rf ./dist && mkdir -p ./dist && tsc --project ./tsconfig.build.json"
  },
  "repository": "github:astrellon/lysithea-vm",
  "keywords": [
    "scripting"
  ],
  "npmName": "lysithea-vm",
  "author": "Alan Lawrey",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
