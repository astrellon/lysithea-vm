{
  "name": "lysithea-language-support",
  "displayName": "Lysithea Language Support",
  "description": "Adds support for Lysithea language",
  "publisher": "astrellon",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "lysithea",
      "aliases": ["Lysithea", "lysithea"],
      "extensions": [".lys", ".lysithea"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "lysithea",
      "scopeName": "source.lysithea",
      "path": "./syntaxes/lysithea.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/astrellon/lysithea-vm.git"
  }
}
