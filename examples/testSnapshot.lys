(function main ()
    (define i 0)
    (define progress "Unset")
    (loop (<= i 10)
        (set progress (printProgress i))
        (++ i)
    )
    (print "Completed at " progress)
)

(function printProgress (i)
    (define progress ($ (* i 10) "%"))
    (print progress)
    (if (== i 5)
        (make-snapshot)
    )
    (return progress)
)

(define testGlobal "global")
(main)